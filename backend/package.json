{
  "name": "smart-campus-backend",
  "version": "1.0.0",
  "description": "Backend API for Smart Campus Instituto - Educational Management Platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:domain": "node test-domain.js",
    "test:repositories": "node test-repositories.js",
    "test:services": "node test-services.js",
    "test:auth": "node test-auth-usecases.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:upload": "node test-upload-document.js",
    "test:validate": "node test-validate-document.js",
    "test:search": "node test-search-documents.js",
    "test:approve-reject": "node test-approve-reject.js",
    "test:documents": "npm run test:upload && npm run test:validate && npm run test:search && npm run test:approve-reject",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:analytics": "node test-analytics-e2e.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "education",
    "nursing",
    "api",
    "rest",
    "smart-campus",
    "healthcare",
    "e-learning",
    "nodejs",
    "express",
    "clean-architecture"
  ],
  "author": "Smart Campus Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.1.3",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.0",
    "sequelize": "^6.35.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs"
}
